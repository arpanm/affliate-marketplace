entity AiToolPayment {
	amount Double
    status PaymentStatus
    paymentLinkUrl String
    pgType PgType
    pgId String
    pgStatus String
    pgRequestJson String
    pgResponseJson String
    pgRequestTimeStamp String
    pgResponseTimeStamp String
    isActive Boolean
    createdBy String
    createdOn LocalDate
    updatedBy String
    updatedOn LocalDate
}

enum PgType {
	RazorPay,
    PayU,
    PayTM
}

enum PaymentStatus {
	Initiate,
    PendingOnPg,
    Success,
    Failed
}

relationship OneToMany {
	AiToolSession to AiToolPayment{session}
}