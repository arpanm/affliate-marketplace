	AiTool to AiToolSession{tool}
}

entity AiToolChat {
	message String
    videoUrl String
    paymentUrl String
    type ChatType
    isFinalVidel Boolean
    isDownloaded Boolean
    isActive Boolean
    createdBy String
    createdOn LocalDate
    updatedBy String
    updatedOn LocalDate
}

enum ChatType {
	UserMessage,
    AiResponse,
    GeneratedVideo,
    PaymentRequest,
    PaymentStatus
